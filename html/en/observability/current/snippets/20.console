PUT _ingest/pipeline/logs-example-default
{
  "description": "Extracts fields and reroutes WARN",
  "processors": [
    {
      "dissect": {
        "field": "message",
        "pattern": "%{@timestamp} %{log.level} %{host.ip} %{message}"
      },
      "reroute": {
        "tag": "high_severity_logs",
         "if" : "$('log.level', '') == 'WARN' || $('log.level', '') == 'ERROR'",
        "dataset": "critical"
      }
    }
  ]
}
